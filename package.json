{
  "name": "whitepearl-manga-assignments",
  "version": "1.0.0",
  "description": "Sistema de asignaciones de manga para WhitePearlTranslations",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.88",
    "@mui/material": "^5.8.6",
    "@mui/x-date-pickers": "^8.10.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@uppy/core": "^3.13.1",
    "@uppy/dashboard": "^3.9.1",
    "@uppy/drag-drop": "^3.1.0",
    "@uppy/file-input": "^3.1.2",
    "@uppy/progress-bar": "^3.1.1",
    "@uppy/react": "^3.4.0",
    "@uppy/status-bar": "^3.3.3",
    "@uppy/xhr-upload": "^3.6.8",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "firebase": "^12.1.0",
    "google-auth-library": "^10.3.0",
    "googleapis": "^159.0.0",
    "imagekit": "^6.0.0",
    "motion": "^12.23.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.0.3",
    "react-hook-form": "^7.33.1",
    "react-hot-toast": "^2.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "react-window": "^2.0.2",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.1.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false react-scripts build",
    "build:strict": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup-firebase": "node setup-firebase.js",
    "setup-firebase-ps": "powershell -ExecutionPolicy Bypass -File setup-firebase.ps1",
    "config": "node setup-firebase.js",
    "worker:setup": "setup-cloudflare-worker.bat",
    "worker:dev": "cd cloudflare-worker && npm run dev",
    "worker:deploy": "cd cloudflare-worker && npm run deploy",
    "worker:secrets": "cd cloudflare-worker && npm run setup-secrets",
    "worker:logs": "cd cloudflare-worker && wrangler tail"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-useless-escape": "warn",
      "eqeqeq": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "cross-env": "^10.0.0"
  }
}
